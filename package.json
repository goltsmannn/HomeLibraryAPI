{
  "name": "homelibraryapi",
  "version": "1.0.0",
  "description": "## Prerequisites",
  "main": ".eslintrc.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goltsmannn/HomeLibraryAPI"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goltsmannn/HomeLibraryAPI/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --testMatch \"<rootDir>/*.spec.ts\" --noStackTrace --runInBand",
    "test:auth": "cross-env TEST_MODE=auth jest --testPathIgnorePatterns refresh.e2e.spec.ts --noStackTrace --runInBand",
    "test:refresh": "cross-env TEST_MODE=auth jest --testPathPattern refresh.e2e.spec.ts --noStackTrace --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "homepage": "https://github.com/goltsmannn/HomeLibraryAPI#readme",
  "dependencies": {
    "@nestjs/common": "^11.0.6",
    "@nestjs/core": "^11.0.6",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "nest": "^0.1.6",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  }
}
